task

a;
function celsiusToFahrenheit(celsius) {
    return (celsius * 9 / 5) + 32;
  }
  console.log(celsiusToFahrenheit(20)); // Output: 68
  

 
  


b;
 var a = 5, b = 10;
 console.log("Before swap: a =", a, "b =", b);
 [a, b] = [b, a];
 console.log("After swap: a =", a, "b =", b);


c;
 function addThreeNumbers(num1, num2, num3) {
    return num1 + num2 + num3;
  }
  console.log(addThreeNumbers(5, 10, 15)); // Output: 30
  
d;
  function celsiusToFahrenheit(celsius) {
    return (celsius * 9 / 5) + 32;
  }
  console.log(celsiusToFahrenheit(20)); // Output: 68

  e;
  function meterToMiles(meter) {
    return meter * 0.000621371;
  }
  console.log(meterToMiles(1000)); // Output: 0.621371
  
f;
  function poundsToKg(pounds) {
    return pounds * 0.453592;
  }
  console.log(poundsToKg(10)); // Output: 4.53592
g;
  function calculateSimpleInterest(principal, rate, time) {
    return (principal * rate * time) / 100;
  }
  console.log(calculateSimpleInterest(1000, 5, 2)); // Output: 100

  
h;
  function areaOfEquilateralTriangle(side) {
    return (Math.sqrt(3) / 4) * side * side;
  }
  console.log(areaOfEquilateralTriangle(5)); // Output: 10.825317547305485
  